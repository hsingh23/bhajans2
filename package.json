  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.21",
    "@fortawesome/free-solid-svg-icons": "5.10.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "bugsnag-js": "4.7.3",
    "bugsnag-react": "^1.1.1",
    "classnames": "^2.2.6",
    "core-js": "^3.2.0",
    "create-react-class": "^15.6.3",
    "firebase": "5.6.0",
    "firebaseui": "3.4.1",
    "jaro-winkler": "^0.2.8",
    "lodash-es": "4.17.15",
    "notie": "^4.3.1",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-ga": "2.6.0",
    "react-highlight-words": "0.16.0",
    "react-hotkeys": "2.0.0",
    "react-modal": "3.9.1",
    "react-paypal-button-v2": "^2.3.2",
    "react-pdf-js": "1",
    "react-router-dom": "5.0.1",
    "react-select": "^3.0.4",
    "react-virtualized": "9.21.1",
    "reactfire": "^1.0.0",
    "reactstrap": "8.0.1",
    "recompose": "0.30.0",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "react-scripts": "3.1.0",
    "react-snap": "^1.23.0",
    "redux-devtools": "3.5.0",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "sw-precache": "5.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && sw-precache --config=sw-precache-config.js && cp ./build/service-worker2.js ./build/service-worker.js",
    "sw-precache": "sw-precache --config=sw-precache-config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postbuild": "react-snap"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook",
      "pre-commit": "lint-staged"
    }
  },
  "reactSnap": {
    "inlineCss": true
  },
  "browserslist": [
    "last 1 version",
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
